{
    "files.exclude": {
        "**/.git": true,
        "**/.DS_Store": true,
        "**/CMakeCache.txt": true,
        "**/cmake_install.cmake": true,
        "**/CMakeFiles": true,
        "**/**.a": true,
        "**/**.elf": true,
        "**/Makefile": true,
        "lib/STM32CubeF0/[^D]*": true,
        "lib/STM32CubeF0/D[^r]*": true,
        "lib/STM32CubeF0/Drivers/BSP": true,
        "lib/STM32CubeF0/Drivers/CMSIS/[^CD]*": true,
        "lib/STM32CubeF0/Drivers/CMSIS/Core_A": true,
        "lib/STM32CubeF0/Drivers/CMSIS/DSP": true,
        "lib/STM32CubeF0/Drivers/CMSIS/Device/ST/STM32F0xx/[^IS]*": true,
        "lib/STM32CubeF0/Drivers/STM32F0xx_HAL_Driver/[^IS]*": true,
        "lib/STM32CubeF0/Drivers/CMSIS/Device/ST/STM32F0xx/Source/Templates/[^gs]*": true,
        "lib/FreeRTOS/[^F]*": true,
        "lib/FreeRTOS/FreeRTOS-Plus": true,
        "lib/FreeRTOS/FreeRTOS+TCP.url": true,
        "lib/FreeRTOS/FreeRTOS/[^S]*": true,
        "lib/FreeRTOS/FreeRTOS/Source/.github": true,
        "lib/FreeRTOS/FreeRTOS/Source/CONTRIBUTING.md": true,
        "lib/FreeRTOS/FreeRTOS/Source/GitHub-FreeRTOS-Kernel-Home.url": true,
        "lib/FreeRTOS/FreeRTOS/Source/History.txt": true,
        "lib/FreeRTOS/FreeRTOS/Source/lexicon.txt": true,
        "lib/FreeRTOS/FreeRTOS/Source/LICENSE.md": true,
        "lib/FreeRTOS/FreeRTOS/Source/Quick_Start_Guide.url": true,
        "lib/FreeRTOS/FreeRTOS/Source/SECURITY.md": true,
        "lib/FreeRTOS/FreeRTOS/Source/portable/[^MG]*": true,
        "lib/FreeRTOS/FreeRTOS/Source/portable/M[^e]*": true,
        "lib/FreeRTOS/FreeRTOS/Source/portable/GCC/[^A]*": true,
        "lib/FreeRTOS/FreeRTOS/Source/portable/GCC/A[^R]*": true,
        "lib/FreeRTOS/FreeRTOS/Source/portable/GCC/ARM[^_]*": true,
        "lib/FreeRTOS/FreeRTOS/Source/portable/GCC/ARM_C[^M]*": true,
        "lib/FreeRTOS/FreeRTOS/Source/portable/GCC/ARM_CM[^0]*": true,
    },
    "C_Cpp.default.compilerPath": "arm-none-eabi-gcc",
    "files.associations": {
        "freertos.h": "c",
        "angle_poll.h": "c",
        "encoder_com.h": "c",
        "main.h": "c",
        "stm32f0xx_hal.h": "c"
    }
}
